Algoritmo "semnome"

     var nota1,nota2,media:real


Inicio
   escreva (" digite a primeira nota ")
   leia(nota1)
   escreva    ("digite a segunda nota")
   leia(nota2)
   
   //media
   media<- nota1 + nota2 /2

   Se (media >= 7) Entao
   escreval("aprovado")
    Senao
    escreval("reprovado")
    FimSe
Fimalgoritmo