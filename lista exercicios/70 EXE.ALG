Algoritmo "70 exe"

var
cont,antes,atual,proximo:inteiro


Inicio


   antes <- 0
   atual <- 1
   proximo <- 1
   

   
      para cont de 1 ate 10 faca
      escreval (proximo)
      proximo <- atual + antes
      antes <- atual
      atual <- proximo
      
   FimPara
      



Fimalgoritmo