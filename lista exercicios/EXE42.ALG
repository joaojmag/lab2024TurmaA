algoritmo "exe42"
var
numero, i: inteiro

inicio

    escreva("Digite um número positivo: ")
    leia(numero)


    se (numero > 0) entao

        para i de 1 ate numero faca
            escreva(i, " ")
        fimPara
        escreva("Acabou!")
    senao
        escreva(" esse valor não é positivo.")
    fimSE
fimalgoritmo
