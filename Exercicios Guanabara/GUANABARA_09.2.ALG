Algoritmo "GUANABARA_09.2"

var

   contador: Inteiro

inicio

      EscrevaL ("CONTAGEM REGRESSIVA")
      EscrevaL ("----------------------")

      contador <- 10

      Enquanto (contador >= 0 ) faca
               EscrevaL(contador)
               contador <- contador - 1
      FimEnquanto
      
      Escreval("")
      Escreval("FIM.")

fimalgoritmo