algoritmo "semnome"

var

contador:inteiro
divisao:inteiro


inicio

contador <- 31

Enquanto (contador >=1)  Faca
 
   contador <- contador - 1


   Se (contador % 4 = 0) Entao
      Escreva("[",contador,"]")
   Senao
      Escreva(contador)

   FimSe

 
FimEnquanto


fimalgoritmo