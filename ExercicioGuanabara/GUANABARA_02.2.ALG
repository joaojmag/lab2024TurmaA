algoritmo "guanabara_02.2"
var

   n1, n2, m: real

inicio

      Escreva("Digite o primeiro número: ")
      Leia(n1)
      Escreva("Digite o segundo número: ")
      Leia(n2)
      
      m <- (n1 + n2)/2
      
      Escreva("A media entre ", n1, " e ", n2, " é ", m)

fimalgoritmo