Algoritmo "GUANABARA_09.1"

var

   contador: Inteiro

inicio

      EscrevaL ("CONTAGEM DE 1 ATÉ 10")
      EscrevaL ("-----------------------")

      contador <- 0

      Enquanto (contador <= 10 ) faca
               EscrevaL(contador)
               contador <- contador + 1
      FimEnquanto
      EscrevaL("")
      EscrevaL( "Fim." )

fimalgoritmo