Algoritmo "GUANABARA_11.4"

var

   cont1, cont2 : Inteiro

inicio

      EscrevaL ("------------------------")
      EscrevaL ("COMBINAÇÕES DO GUANABARA")
      EscrevaL ("------------------------")
      EscrevaL (" ")

      Para cont1 := 1 ate 3 faca
           Para cont2 := 1 ate 3 faca
                EscrevaL(cont1, cont2)
           FimPara
      FimPara

fimalgoritmo