algoritmo "semnome"
var
contador,numero,tabuada:inteiro
inicio
Escreval("DIGITE UM NUMERO PARA LERMOS A TABUADAD DELE!")
Escreval("==============================================")
Leia(numero)

contador <- 1

para contador de 1 ate 10 faca
   tabuada <- numero * contador
   Escreval(numero,"X",contador,"=",tabuada)
fimpara

fimalgoritmo
