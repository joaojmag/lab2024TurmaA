          algoritmo "Contagem Regressiva"
var
contador: inteiro

inicio
    contador := 100

    // Laço para contagem regressiva de 100 até 0
    enquanto contador >= 0 faca
        escreva(contador, " ")
        contador := contador - 5
fimEnquanto

    // Exibe "Acabou!" após a contagem regressiva
    escreva("Acabou!")
fimalgoritmo
