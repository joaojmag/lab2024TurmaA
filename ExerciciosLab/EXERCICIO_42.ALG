algoritmo "exercicio_42"
var

   contador, numero: inteiro

inicio

      contador  <- 1
      Escreva("ME INFORME UM NÚMERO INTEIRO E POSITIVO: ")
      Leia(numero)
      Escreval(" ")
      
      Enquanto (contador <= numero) faca
      Escreva(contador)
      contador <- contador + 1
      FimEnquanto
      Escreval(" ")
fimalgoritmo