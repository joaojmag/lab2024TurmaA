algoritmo "exercicio_41"
var

   programa: inteiro

inicio

      Escreval("CONTADOR.")
      Escreval("------------------------")
      programa <- 100
      Enquanto (programa >= 0) faca
      Escreva(programa)
      programa <- programa - 5
      FimEnquanto
      Escreval(" ")
      Escreva("Acabou!")

fimalgoritmo