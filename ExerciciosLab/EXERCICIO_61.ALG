algoritmo "exercicio_61"
var

   cont, contagem: inteiro

inicio

      cont <- 0
      Enquanto (cont <= 30) e (contagem <= 30) faca
               Escreval(contagem)
               contagem <- contagem + 3
               cont <- cont + 1
      fimenquanto

      Escreva("")
      Escreva("Acabou!")

fimalgoritmo