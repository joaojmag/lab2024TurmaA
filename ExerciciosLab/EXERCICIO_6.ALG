algoritmo "exercicio_6"
var
   numero, antecessor, sucessor: Inteiro
inicio
      Escreva("Informe um número: ")
      Leia(numero)
      antecessor <- numero-1
      sucessor <- numero+1
      Escreval("O antecessor do numero é ", antecessor)
      Escreva("O sucessor do numero é ", sucessor)
fimalgoritmo