algoritmo "exercicio_43"
var

   contador: inteiro

inicio

      contador <- 30
      Enquanto (contador >= 0) faca
      Escreval(contador)
      contador <- contador -1
      se (contador % 4 = 0) entao
      Escreva("[",contador,"]")
      contador <- contador - 1
      EscrevaL("")
      fimse
      fimenquanto

fimalgoritmo