algoritmo "Exer-18"
var
 ano, idade:inteiro
inicio
      Escreva ("Qual seu ano de nascimento?")
      leia (ano)
      idade <- 2024 - ano
      se idade >=18 entao
         Escreva ("você pode votar")
      senao
         Escreva ("você não pode votar")

      fimSe


fimalgoritmo
