Algoritmo "Exer-48"

Var
 contador, valor, soma: inteiro

Inicio
    contador <- 1
    
    Enquanto (contador <= 7) faca
             escreva ("Digite o", contador,"° valor: ")
             leia (valor)

             soma <- soma+valor
             contador <- contador +1

    fimEnquanto

    escreva ("A soma de todos os  valores é: ",soma)


Fimalgoritmo