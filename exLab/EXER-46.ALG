Algoritmo "Exer-46"

Var
  contador, valor, soma: inteiro

Inicio
    escreval ("VAMOS CALCULAR:")
    
    contador <- 6

    Enquanto (contador <= 100) faca
             escreval (contador)
             soma <- soma + contador
             contador <- contador + 2
    fimEnquanto
    
    escreval ("A soma de todos os  valores é: ",soma)


Fimalgoritmo